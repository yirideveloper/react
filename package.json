{
  "name": "reactotron",
  "version": "0.1.0",
  "description": "A console-based remote control for React Native.",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "docs": "node_modules/.bin/jsdoc --configure .jsdoc.json",
    "dist": "npm run clean && gulp build && npm run docs",
    "clean": "rm -rf dist docs",
