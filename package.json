  "files": [
    "dist/reactotron-server.js",
    "dist/reactotron-client.js"
  ],
  "dependencies": {
    "blessed": "^0.1.81",
    "moment": "^2.12.0",
    "ramda": "^0.21.0",
    "ramdasauce": "^1.0.0",
    "socket.io": "^1.4.5"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "docdash": "^0.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-rollup": "^1.8.0",
    "jsdoc": "^3.4.0",
    "rollup": "^0.25.8",
